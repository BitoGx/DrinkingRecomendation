<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="id.web.bitocode.drinkingrecomendation">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
      android:name=".UpdateProfilActivity"
      android:parentActivityName=".DashboardActivity"/>
    <activity
      android:name=".GantiPasswordActivity"
      android:parentActivityName=".DashboardActivity"/>
    <activity
      android:name=".SliderActivity"
      android:theme="@style/AppTheme.NoActionBar">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
      android:name=".LoginActivity"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".RegisterActivity"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".LupaPasswordActivity"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".DashboardActivity"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".ActivityRecognitionActivity"
      android:parentActivityName=".DashboardActivity" />
    <activity
      android:name=".DashboardsActivity"
      android:theme="@style/AppTheme.NoActionBar" />

    <service
      android:name=".DetectedActivitiesIntentService"
      android:exported="false" />
    <service android:name=".BackgroundDetectedActivitiesService" />
  </application>
  <uses-permission android:name="com.google.android.gms.permission.ACTIVITY_RECOGNITION" />

  <uses-permission android:name="android.permission.INTERNET" />

</manifest>